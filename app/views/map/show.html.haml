#car_washes_left_list
  #list_header
    %p СПИСОК
    = image_tag 'timer.png'
  %ul#car_wash_list
    -@car_washes.each do |cw|
      %li= link_to cw.title, car_wash_path(cw)
  #adv_left
    = image_tag 'adv_left.png'
    = image_tag 'adv_left.png'
    = image_tag 'adv_left.png'

#map_container
  %h3.text-center#map_header Карта автомоек Москвы
  #map_search
    = form_for '/search', method: 'get', remote: true do |f|
      = f.search_field 'address_search', placeholder: "Введите адрес..." 
      = f.submit t("search_button"), class: 'submit'
  #map
